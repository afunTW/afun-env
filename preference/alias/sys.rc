# ##############################
# Sys alias
# ##############################

# Navigation
alias -="cd -"
alias ...=../..
alias ....=../../..
alias .....=../../../..
alias ......=../../../../..
alias 1="cd -"
alias 2="cd -2"
alias 3="cd -3"
alias 4="cd -4"
alias 5="cd -5"
alias 6="cd -6"
alias 7="cd -7"
alias 8="cd -8"
alias 9="cd -9"
alias _="sudo "

# File Manager
alias l="ls -lah"
alias la="ls -lAh"
alias ll="ls -lh"
alias ls="ls --color=tty"
alias lsa="ls -lah"
alias md="mkdir -p"
